{
  "css.customData": [".vscode/css_custom_data.json"],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "typescript": "typescriptreact",
    "javascript": "javascriptreact"
  },
  "tailwindCSS.experimental.classRegex": [
    "class\\w*\\s*:?\\s*\\"([^\\"]*)\\"",
    "class\\w*\\s*:?\\s*'([^']*)'",
    "class\\w*\\s*:?\\s*`([^`]*)`"
  ]
}
